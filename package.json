{
  "name": "vscode-js-html-highlight",
  "version": "0.0.2",
  "displayName": "HTML Highlighter for template strings in JS/TS",
  "description": "HTML Highlighter for template strings in JS/TS",
  "author": "Anton Nyman <anton@antonnyman.se>",
  "publisher": "antonnyman",
  "icon": "img/brackets.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonnyman/vscode-js-html-highlight.git"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/js-html-highlight.json",
        "scopeName": "source.js.embedded.html",
        "injectTo": [
          "source.js",
          "source.ts"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.html": "html"
        }
      }
    ]
  }
}